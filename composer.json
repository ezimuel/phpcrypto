{
    "name": "ezimuel/phpcrypto",
    "description": "Cryptographic library for PHP 7 based on OpenSSL",
    "license": "BSD-3-Clause",
    "keywords": [
        "Cryptography",
        "PHP7"
    ],
    "homepage": "https://github.com/ezimuel/phpcrypto",
    "autoload": {
        "psr-4": {
            "PHPCrypto\\": "src/"
        }
    },
    "require": {
        "php": ">=7.0",
        "ext-openssl": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "PHPCryptoTest\\": "test/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    }
}
